n  emit('onItemClick', componentData)\r\n}\r\ndefineExpose({\r\n  // onListClick,\r\n  onItemClick,\r\n  onImageUpoded\r\n})\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n// .create-component-list {\r\n//   display: flex;\r\n//   flex-direction: column;\r\n//   justify-content: center;\r\n// }\r\n.component-item {\r\n  width: 100px;\r\n  margin: 0 auto;\r\n  margin-bottom: 15px;\r\n}\r\n.component-item > * {\r\n  position: static !important;\r\n}\r\n</style>\r\n"]}]}